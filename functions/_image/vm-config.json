{
  "boot-source": {
    "kernel_image_path": "/kernels/linux.git/vmlinux",
    "boot_args": "console=ttyS0 reboot=k panic=1 pci=off"
  },
  "drives": [
    {
      "drive_id": "rootfs",
      "is_root_device": false,
      "cache_type": "Unsafe",
      "is_read_only": true,
      "path_on_host": "/filesystems/rootfs.ext4",
      "io_engine": "Sync"
    }
  ],
  "machine-config": {
    "vcpu_count": 2,
    "mem_size_mib": 256
  },
  "network-interfaces": []
}
